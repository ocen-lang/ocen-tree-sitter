{
  "$schema": "https://tree-sitter.github.io/tree-sitter/assets/schemas/config.schema.json",
  "grammars": [
    {
      "name": "ocen",
      "camelcase": "Ocen",
      "title": "Ocen",
      "scope": "source.ocen",
      "file-types": [
        "ocen",
        "oc"
      ],
      "injection-regex": "^ocen$",
      "class-name": "TreeSitterOcen"
    }
  ],
  "metadata": {
    "version": "0.1.0",
    "license": "MIT",
    "description": "Ocen grammar for tree-sitter",
    "authors": [
      {
        "name": "Mustafa Quraish",
        "email": "mustafaquraish@gmail.com"
      }
    ],
    "links": {
      "repository": "https://github.com/ocen-lang/ocen-tree-sitter"
    }
  },
  "bindings": {
    "c": true,
    "go": true,
    "node": true,
    "python": true,
    "rust": true,
    "swift": true,
    "zig": false
  }
}